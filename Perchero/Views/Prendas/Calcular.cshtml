                            <td>@item.MetroTela <input type="hidden" name="DetallePrendas[@i].MetroTela" value="@item.MetroTela" /> </td>
                        }
                        else
                        {
                            <td></td>
                            <td></td>
                        }
                        @if (item.Avio != null)
                        {
                            <td>@item.Avio.Nombre <input type="hidden" name="DetallePrendas[@i].AvioId" value="@item.AvioId" /></td>
                            <td>@item.CantidadAvio <input type="hidden" name="DetallePrendas[@i].CantidadAvio" value="@item.CantidadAvio" /></td>
                        }
                        else
                        {
