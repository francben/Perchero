@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script>
        $(function () {
            //////
            var to = 0;
            var tt = 0;
            var temp = [];
            var Tprecio = "";
            var Pprecio = $("#PrecioTotal").val();
            to = Pprecio.length;
            tt = to;
            for (var i = 0; i < to; i++) {
                temp[tt] = Pprecio[i];
                if (i == 2) {
                    tt -= 1
                    temp[tt] = "."
                }
                tt -= 1;
            }
            for (var i = 0; i < temp.length; i++) {
                Tprecio = Tprecio + temp[to];
                to -= 1
            }
            $("#VistaPrecio").text("Gs." + Tprecio);

            ////
            $("#Imagen").fileinput({ showCaption: false, showUpload: false, mainClass: "input-group-lg" });

            cargar_talle();
            function cargar_talle() {
                var codigoTalle = {
                    url: "/Talles/getTalles",
                    getValue: "NumeroTalle",
                    list: {
                        onSelectItemEvent: function () {
                            var TalleId = $(".TalleC").getSelectedItemData().Id;
                            $("#TalleId").val(TalleId).trigger("change");
                            var NumeroTalle = $(".TalleC").getSelectedItemData().NumeroTalle;
                            $("#NumeroTalle").val(NumeroTalle)
                            //$("#Talle").val(NumeroTalle)
                            var AlturaAxila = $(".TalleC").getSelectedItemData().NumeroTalle;
                            $("#AlturaAxila").val(AlturaAxila)
                            var AlturaBusto = $(".TalleC").getSelectedItemData().AlturaBusto;
                            $("#AlturaBusto").val(AlturaBusto)
                            var AlturaCCadera = $(".TalleC").getSelectedItemData().AlturaCCadera;
